        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": [ "websiteCategoryMs.all" ]
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:9001"
  }
}