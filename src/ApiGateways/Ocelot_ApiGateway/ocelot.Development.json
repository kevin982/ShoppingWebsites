{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/v1/WebsiteCategory",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 9003
        }
      ],
      "UpstreamPathTemplate": "/v1/WebsiteCategory",
      "UpstreamHttpMethod": [
        "Get",
        "Post"
      ],
